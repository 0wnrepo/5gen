# CLT experiment for 10^12 for 3DNF, lambda = 80

LAMBDA=80
BASE=4
LENGTH=8

cp samples/base-$BASE-length-$LENGTH-3dnf.json public/template.json

date
./keygen -s $LAMBDA -n 8 -C
date
./3dnf_encrypt -i 1 -C $BASE $LENGTH 11
date
./eval -C '{"a":"1","b":"1","c":"1"}'
date

du -sh public
du -sh private
du -sh database

